{
  "expo": {
    "name": "Challengers",
    "slug": "Challengers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/screens/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/screens/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#038F00"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/screens/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}